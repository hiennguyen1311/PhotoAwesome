{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": [
      "es6"
    ],
    "baseUrl": "./",
    "paths": {
      "@asset/*": [
        "src/asset/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@constant/*": [
        "src/constant/*"
      ],
      "@model/*": [
        "src/model/*",
      ],
      "@screen/*": [
        "src/screen/*"
      ],
      "@i18n/*": [
        "src/i18n/*"
      ],
      "@component/*": [
        "src/component/*"
      ],
      "@service/*": [
        "src/service/*"
      ],
      "@redux/*": [
        "src/redux/*",
      ],
      "@util/*": [
        "src/util/*"
      ],
    },
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "exclude": [
    "node_modules"
  ]
}